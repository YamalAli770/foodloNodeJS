<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>foodlo.pk</title>
    <!-- CSS -->
    <link rel="stylesheet" type="text/css" href="/css/home.css">
    <link rel="stylesheet" type="text/css" href="/css/cart.css">
    <!-- Fonts Awesome CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Mono:wght@500&display=swap" rel="stylesheet">
    <title>foodlo.pk</title>
    <!-- Carousel -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" integrity="sha512-tS3S5qG0BlhnQROyJXvNjeEM4UpMXHrQfTGmbQ1gKmelCxlSEBUaxhRBj/EFTzpbP4RVSrpEikbmdJobCvhE3g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css" integrity="sha512-sMXtMNL1zRzolHYKEujM2AqCLUR9F2C4/05cdbxjjLSRvMQIciEPCQZo++nk7go3BtSuK9kfa/s+a4f4i5pLkw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- Carousel End -->

    <!-- Mapbox -->
    <script src='https://api.mapbox.com/mapbox-gl-js/v2.8.1/mapbox-gl.js'></script>
    <link href='https://api.mapbox.com/mapbox-gl-js/v2.8.1/mapbox-gl.css' rel='stylesheet' />
    <!-- Mapbox End -->
</head>
<body>
    <!-- Hamburger -->
    <aside class="sidebar">
        <div class="sidebar-header">
            <img src="/images/foodlo Logo/logo1.png" alt="" class="logo">
            <button class="close-btn"><i class="fa-solid fa-xmark"></i></button>
        </div>
        <ul class="links">
            <li><a href="/">Home</a></li>
            <li><a href="/about">About</a></li>
            <li><a href="/#gallery">Gallery</a></li>
            <li><a href="/products">Order Now</a></li>
        </ul>
        <div class="cart-btn-wrapper">
            <a href="" class="cart">Cart <i class="fas fa-shopping-cart"></i></a>
            <a href="" class="login">Login <i class="fa-solid fa-user-plus"></i></a>
        </div>
    </aside>

    <button class="back-to-top hidden"><i class="fa-solid fa-circle-arrow-up"></i></button>

    <header class="header" id="myHeader">
        <div class="headLogo">
            <a href="/"><img src="/images/foodlo Logo/logo1.png" alt="foodlo Logo"></a>
        </div>
        <nav class="nav" id="nav">
            <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/about">About</a></li>
                <li><a href="/#gallery">Gallery</a></li>
                <li><a href="/products">Order Now</a></li>
            </ul>
        </nav>
        <ul class="cart-login" id="cart-login">
            <li><a href="">Cart <i class="fas fa-shopping-cart"></i></a></li>
            <li><a href="/register">Register/Login <i class="fa-solid fa-user-plus"></i></a></li>
        </ul>
        <button class="open-btn"><i class="fa-solid fa-bars"></i></button>
    </header>

    <!-- Cart Section -->

    <h1 id="cartTopHeader">Shopping Cart</h1>
    <section class="cartProductContainer">
        <div class="cartContainer">
            <div class="leftCartChild">
                <div class="leftCartTop">
                    <h1 class="cartHeader">Cart Items</h1>
                </div>
                <div class="leftCartBottom cartProduct">
                    <div class="bottomFirst">
                        <img src="/images/neapolitan-pizza.jpg" alt="">
                    </div>
                    <div class="bottomSecond">
                        <h3 class="cartProductName">Neapolitan Pizza</h2>
                        <p class="cartProductQty">Quantity: <button class="subtractQty">-</button> 1 <button class="addQty">+</button> </p>
                        <div class="btnAndPrice">
                            <button class="removeItemFromCart">Remove Item</button>
                            <div class="cartProductPrice">$14.99</div>
                        </div>
                    </div>
                </div>
                <div class="cartProductSeperator"></div>
                <div class="leftCartBottom cartProduct">
                    <div class="bottomFirst">
                        <img src="/images/pasta.png" alt="">
                    </div>
                    <div class="bottomSecond">
                        <h3 class="cartProductName">Nonna's Pasta</h2>
                        <p class="cartProductQty">Quantity: <button class="subtractQty">-</button> 1 <button class="addQty">+</button> </p>
                        <div class="btnAndPrice">
                            <button class="removeItemFromCart">Remove Item</button>
                            <div class="cartProductPrice">$14.99</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="rightCartChild">
                <div class="rightCartTop">
                    <h2 class="rightCartHeader">Cart Totals</h2>
                </div>
                <div class="rightCartMiddle">
                    <div class="cartAmount">
                        <h5>Amount</h5>
                        <p>$14.99</p>
                    </div>
                    <div class="cartShipping">
                        <h5>Shipping Fee</h5>
                        <p>$3</p>
                    </div>
                </div>
                <div class="rightCartBottom">
                    <p><strong>Total</strong><span>(Including VAT)</span></p>
                    <p>$17.99</p>
                </div>
                <div class="checkoutButtonContainer">
                    <a href="/checkout"><button class="checkoutButton">Checkout</button></a>
                </div>
            </div>
        </div>
    </section>

    <!-- Footer -->

    <footer class="footer">
        <div class="firstGridChild">
            <div class="logo">
                <img src="/images/foodlo Logo/logo1.png" alt="">
            </div>
            <div class="socials">
                <a href="https://www.facebook.com/" target="_blank"><i class="socialIcon fa-brands fa-facebook-f"></i></a>
                    <a href="https://www.instagram.com/"><i class="socialIcon fa-brands fa-instagram"></i></a>
                    <a href="https://www.twitter.com/"><i class="socialIcon fa-brands fa-twitter"></i></a>
                    <a href="https://www.tiktok.com/"><i class="socialIcon fa-brands fa-tiktok"></i></a>
            </div>
        </div>
        <div class="secondThirdGridContainer">
            <div class="secondGridChild">
                <ul>
                    <li><h3>Support</h3></li>
                    <li><a href="">Contact Us</a></li>
                    <li><a href="">FAQ</a></li>
                    <li><a href="">Branch Locator</a></li>
                </ul>
            </div>
            <div class="thirdGridChild">
                <ul>
                    <li><h3>foodlo</h3></li>
                    <li><a href="">About FoodLo</a></li>
                    <li><a href="">Carrers</a></li>
                    <li><a href="">Newsroom</a></li>
                </ul>
            </div>
        </div>
        <div class="fourthGridChild" id="map"></div>
        <div class="fifthGridChild">
            <p>Copyright &copy; 2022 FoodLo pk | All Rights Reserved</p>
        </div>
    </footer>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js" integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="/js/index.js"></script>
</body>
</html>